{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "db",
    "options": {
      "postgresql": {
        "schema": "schema_name"
      }
    },
    "acls": [
      {
        "accessType": "READ",
        "principalType": "ROLE",
        "principalId": "$owner",
        "permission": "ALLOW",
        "property": "*"
      }
    ]
  },
  "AccessToken": {
    "dataSource": "db",
    "public": false,
    "options": {
      "postgresql": {
        "schema": "schema_name"
      }
    }
  },
  "ACL": {
    "dataSource": "db",
    "public": false,
    "options": {
      "postgresql": {
        "schema": "schema_name"
      }
    }
  },
  "RoleMapping": {
    "dataSource": "db",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true,
      "postgresql": {
        "schema": "schema_name"
      }
    }
  },
  "Role": {
    "dataSource": "db",
    "public": false,
    "options": {
      "postgresql": {
        "schema": "schema_name"
      }
    }
  }
}