      "type": "belongsTo",
      "model": "elderly",
      "foreignKey": "elderlyId"
    },
    "ingredients": {
      "type": "hasMany",
      "model": "ingredient",
      "foreignKey": "shoppingListId",
      "through": "shoppingListIngredients",
      "keyThrough": "ingredientId"
    }
  },
  "acls": [],
  "methods": {}
}